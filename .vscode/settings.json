{
  "[cpp]": {
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    }
  },
  "[c]": {
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    }
  },
  "cmake.configureOnOpen": false,
  "cmake.generator": "Ninja",
  "editor.formatOnSave": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    ".scannerwork": true,
    "build/": true,
    "build_wrapper_output": true,
    "debug.log": true,
    "dependencies.lock": true,
    "partition_table": true,
    "sdkconfig": true,
    "sdkconfig.old": true,
    "test/build/": true,
    "test/sdkconfig.old": true,
    "test/sdkconfig": true,
    "test/dependencies.lock": true
  },
  "idf.adapterTargetName": "esp32",
  "idf.flashBaudRate": "115200",
  "idf.flashType": "UART",
  "idf.openOcdConfigs": [
    "interface/cjmcu-ftdi-2232HL.cfg",
    "board/esp-wroom-32.cfg"
  ],
  "idf.port": "/dev/ttyUSB1",
  "idf.portWin": "COM3",
  "sonarlint.pathToCompileCommands": "${workspaceFolder}/build/compile_commands.json"
}
