      "build/": true,
      "debug.log": true,
      "sdkconfig.old": true,
      "test/build/": true,
      "test/sdkconfig.old": true
    },
    "files.associations": {
      "sdkconfig": "c",
      "cstddef": "cpp",
      "limits": "cpp",
      "memory": "cpp",
      "mutex": "cpp",
      "vector": "cpp",
      "xhash": "cpp",
      "xtree": "cpp",
      "xutility": "cpp",
